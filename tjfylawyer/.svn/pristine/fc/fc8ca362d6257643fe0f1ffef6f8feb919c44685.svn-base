/*chldd*/
.daterange {
	float: right;
	display: inline-block;
	margin: 10px 5px 0 0;
	cursor: pointer;
}

.daterange span {
	float: left;
}

.sqlx_wrapper {
	display: inline-block;
	margin: 10px 0 0 0;
}

.sqlx_wrapper span {
	display: inline-block;
	margin-right: 5px;
}

.sqlx_wrapper input {
	position: relative;
	top: 2px;
}

.leftmenu dd span {
	height: 16px;
	margin-top: 4px;
}
/*clsqxq*/
.table_wrapper table {
	line-height: 2;
	margin-left: 50px;
}

.table_wrapper table tbody td {
	padding-top: 5px;
}

.table_wrapper table tbody .checkboxlist {
	max-width: 300px;
}
/* .table_wrapper table input,select{
	width:100%;
	height: 25px;
} */
.table_wrapper table input {
	width: 100%;
	height: 25px;
}

.table_wrapper table input.i_checkbox { /* width:initial;
	height: initial; */
	width: 15px;
	height: 15px;
	margin-right: 5px;
	vertical-align: sub;
}

.table_wrapper span {
	margin-left: 5px;
	height: 25px;
	line-height: 25px;
}
.table_date_middle{
	text-align:center;
}

.formbody {
	padding: 10px 18px;
}

.formtitle {
	border-bottom: solid 1px #d0dee5;
	line-height: 35px;
	position: relative;
	height: 35px;
	margin-bottom: 28px;
}

.formtitle span {
	font-weight: bold;
	font-size: 14px;
	border-bottom: solid 3px #66c9f3;
	float: left;
	position: absolute;
	z-index: 1;
	bottom: -1px;
	padding: 0 3px;
	height: 30px;
	line-height: 30px;
}

.formtitle_toolbar{
	position: absolute;
	top: 45px;
	right: 25px;
}

.forminfo {
	padding-left: 23px;
}

.forminfo li {
	margin-bottom: 13px;
	clear: both;
}

.forminfo li label {
	width: 86px;
	line-height: 34px;
	display: block;
	float: left;
}

.forminfo li i {
	color: #7f7f7f;
	padding-left: 20px;
	font-style: normal;
}

.forminfo li cite {
	display: block;
	padding-top: 10px;
}

.dfinput {
	width: 100%;
	height: 25px;
	line-height: 25px;
	border-top: solid 1px #a7b5bc !important;
	border-left: solid 1px #a7b5bc !important;
	border-right: solid 1px #ced9df !important;
	border-bottom: solid 1px #ced9df !important;
	background: url(../images/inputbg.gif) repeat-x;
	text-indent:4px;
}

.textinput {
	border-top: solid 1px #a7b5bc;
	border-left: solid 1px #a7b5bc;
	border-right: solid 1px #ced9df;
	border-bottom: solid 1px #ced9df;
	background: url(../images/inputbg.gif) repeat-x;
	padding: 10px;
	width: 504px;
	height: 135px;
	line-height: 20px;
	overflow: hidden;
}

.btn {
	width: 137px;
	height: 35px;
	background: url(../images/btnbg.png) no-repeat;
	font-size: 14px;
	font-weight: bold;
	color: #fff;
	cursor: pointer;
}

.btn_wrapper {
	margin: 10px 0 0 55px;
}

.yddcl_table,.table_yddcl,.table_xgsq,.table_bcsq {
	height: 100%;
	width: 100%;
	margin-right: 2px;
	text-align: center;
}

.yddcl_table table.tablelist {
	width: 98%;
}

.yddcl_table .title {
	float: left;
}
.picTip{
	margin:10px 0;
}
.picTip li{
	display:inline-block;
	margin-left:10px;
}
.picTip li img{
	float:left;
	margin:-2px 5px 0 10px;
}
.tablelist img{
	vertical-align: middle;
}
.imgList{
	display: inline-block;
	width: 100%;
	height:330px;
	overflow-y:auto;
}
.imgList ul{
	margin-top:5px;
}
.img_wrapper{
	display: inline-block;
	float: left;
	width: 123px;
	height: 130px;
	margin: 15px 10px;
	border: 1px solid rgb(221,221,221);
}
.img_info{
	display: inline-block;
	float: left;
	margin: 10px 0 0 10px;
	font-weight: bold;
}
.img_state{
	width: 55px;
	height: 31px;
	background: url(../images/clzt1.png) 0 0 no-repeat;
	float: right;
	margin: -10px -10px 0px 0px;
}
.img_pic{
	width: 115px;
	height: 80px;
	position: relative;
	top: 20px;
	left: 4px;
}